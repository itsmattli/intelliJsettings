<templateSet group="REW">
  <template name="&lt;?= __(' '); ?&gt;" value="&lt;?= __('$SELECTION$'); ?&gt;" description="Text Setter" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="HTML" value="true" />
    </context>
  </template>
  <template name="Pretty Dump" value="echo &quot;&lt;pre&gt;&quot;;&#10;echo var_dump($SELECTION$);&#10;echo &quot;&lt;/pre&gt;;" description="Bad Debugging" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="HTML" value="true" />
      <option name="HTML_TEXT" value="false" />
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="__( ):" value="__($SELECTION$)" description="Text Setter" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="__(' ' );" value="__('$SELECTION$')" description="Text Setter" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="// eslint-disable-line no-console" value="$SELECTION$ // eslint-disable-line no-console" description="" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="{% trans ' '  %}" value="{% trans '$SELECTION$' %}" description="Text Setting" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="Twig" value="true" />
    </context>
  </template>
  <template name="{% trans %}" value="{% trans $SELECTION$ %}" description="" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="Twig" value="true" />
    </context>
  </template>
  <template name="{% trans %} {% endtrans %}" value="{% trans %}$SELECTION${% endtrans %}" description="" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="Twig" value="true" />
    </context>
  </template>
</templateSet>